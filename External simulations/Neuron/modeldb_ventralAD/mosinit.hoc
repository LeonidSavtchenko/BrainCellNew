Vrest=-70
proc init() {

  t=0
  
  finitialize(Vrest)

	forall {
		v = Vrest
		if(ismembrane("nax") || ismembrane("na3")) {
			ena=50
		}
		if(ismembrane("kdr") || ismembrane("kap") || ismembrane("kad") || ismembrane("kmb") || ismembrane("kir")) {
			ek=-90
		}
	if(ismembrane("hdpas")) {
			ehd_hd=-30
		}
		
	}

	finitialize(Vrest)
	fcurrent()
	cvode.re_init()

}



